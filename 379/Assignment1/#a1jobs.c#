#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include <unistd.h>
#include <time.h>



int main(){
  int state = 1;
  char input[20];
  int pid = getpid();
  time_t cur_time;
  /* Get current time*/
  cur_time = time(NULL);
  
  
  while(state != 0){
    printf("Xuan[%i]:",pid);
    state = 0;
  }

  printf("C program\n");
  return 0;


}






